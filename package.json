{
  "name": "notifications-admin",
  "version": "0.0.1",
  "description": "Admin front end for GOV.UK Notify",
  "engines": {
    "node": ">=10.15.3"
  },
  "scripts": {
    "test": "npm run lint:scss && npm run lint:js && jest --config tests/javascripts/jest.config.js tests/javascripts",
    "test-watch": "jest --watch --config tests/javascripts/jest.config.js tests/javascripts",
    "build": "gulp",
    "watch": "gulp watch",
    "audit": "better-npm-audit audit --production --level high",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "stylelint --fix \"**/*.scss\"",
    "lint:js": "eslint",
    "lint:js:fix": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphagov/notifications-admin.git"
  },
  "author": "Government Digital Service",
  "license": "MIT",
  "homepage": "https://github.com/alphagov/notifications-admin#readme",
  "dependencies": {
    "cbor-js": "0.1.0",
    "date-fns": "^3.6.0",
    "govuk-frontend": "5.6.0",
    "hogan": "1.0.2",
    "jquery": "3.5.0",
    "leaflet": "1.6.0",
    "morphdom": "2.6.1",
    "proj4": "^2.11.0",
    "query-command-supported": "1.0.0",
    "textarea-caret": "3.1.0",
    "timeago": "1.6.5"
  },
  "devDependencies": {
    "@babel/core": "7.24.4",
    "@babel/preset-env": "7.24.4",
    "@eslint/js": "9.11.1",
    "@rollup/plugin-multi-entry": "6.0.1",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@rollup/plugin-terser": "0.4.4",
    "better-npm-audit": "3.7.3",
    "eslint": "9.11.1",
    "eslint-plugin-jest": "28.8.3",
    "globals": "15.10.0",
    "jest": "29.7.0",
    "jest-each": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "postcss-replace": "2.0.1",
    "rollup": "4.24.0",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-styler": "1.8.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "sass": "1.32.7",
    "stylelint": "16.9.0",
    "stylelint-config-gds": "2.0.0",
    "stylelint-config-standard-scss": "13.1.0",
    "streamqueue": "1.1.2"
  },
  "overrides": {
    "minimist": "1.2.8",
    "lodash.template": "4.5.0"
  }
}
